{
	"geode": "4.3.1",

	"gd": {
		"win": "2.2074",
		"android": "2.2074",
		"mac": "2.2074",
		"ios": "2.2074"
	},

	"id": "trealdry.dynamic-icon-change",
	"name": "Dynamic Icon Change",
	"version": "v0.4.2",
	"developers": ["TrealDry", "MrKolii"],
	"description": "Icon change when going through portal.",

	"tags": ["customization", "offline"],

	"links": {
		"source": "https://github.com/TrealDry/Dynamic-Icon-Change", 
		"community": "https://discord.gg/qFMn5VvwCB"
	},

	"dependencies": {
		"geode.node-ids": {
			"version": ">=1.19.0",
			"importance": "required"
		}
	},

	"settings": {
		"main-settings": {
			"type": "title",
			"name": "Main settings"
		},

		"disable-mod": {
			"type": "bool",
			"name": "Disable mod",
			"default": false
		},

		"unlock-icons": {
			"type": "bool",
			"name": "Use only unlocked icons",
			"default": false
		},

		"range-settings": {
			"type": "title",
			"name": "Range settings"
		},

		"min-range-cube": {
			"type": "int",
			"name": "Min range of cube icons",
			"default": 1,
			"min": 1,
			"max": 485
		},
		"max-range-cube": {
			"type": "int",
			"name": "Max range of cube icons",
			"default": 485,
			"min": 1,
			"max": 485
		},

		"min-range-ship": {
			"type": "int",
			"name": "Min range of ship icons",
			"default": 1,
			"min": 1,
			"max": 169
		},
		"max-range-ship": {
			"type": "int",
			"name": "Max range of ship icons",
			"default": 169,
			"min": 1,
			"max": 169
		},

		"min-range-ball": {
			"type": "int",
			"name": "Min range of ball icons",
			"default": 1,
			"min": 1,
			"max": 118
		},
		"max-range-ball": {
			"type": "int",
			"name": "Max range of ball icons",
			"default": 118,
			"min": 1,
			"max": 118
		},
		
		"min-range-ufo": {
			"type": "int",
			"name": "Min range of ufo icons",
			"default": 1,
			"min": 1,
			"max": 149
		},
		"max-range-ufo": {
			"type": "int",
			"name": "Max range of ufo icons",
			"default": 149,
			"min": 1,
			"max": 149
		},

		"min-range-wave": {
			"type": "int",
			"name": "Min range of wave icons",
			"default": 1,
			"min": 1,
			"max": 96
		},
		"max-range-wave": {
			"type": "int",
			"name": "Max range of wave icons",
			"default": 96,
			"min": 1,
			"max": 96
		},

		"min-range-robot": {
			"type": "int",
			"name": "Min range of robot icons",
			"default": 1,
			"min": 1,
			"max": 68
		},
		"max-range-robot": {
			"type": "int",
			"name": "Max range of robot icons",
			"default": 68,
			"min": 1,
			"max": 68
		},

		"min-range-spider": {
			"type": "int",
			"name": "Min range of spider icons",
			"default": 1,
			"min": 1,
			"max": 69
		},
		"max-range-spider": {
			"type": "int",
			"name": "Max range of spider icons",
			"default": 69,
			"min": 1,
			"max": 69
		},
		
		"min-range-swing": {
			"type": "int",
			"name": "Min range of swing icons",
			"default": 1,
			"min": 1,
			"max": 43
		},
		"max-range-swing": {
			"type": "int",
			"name": "Max range of swing icons",
			"default": 43,
			"min": 1,
			"max": 43
		}
	}
}
